<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/title_finalizar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:textAlignment="center"
        android:textSize="20dp"
        android:text="@string/title_finalizar"/>

    <TextView
        android:id="@+id/votos_totais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title_finalizar"
        android:layout_marginTop="50dp"
        android:textAllCaps="true"
        android:textStyle="italic"
        android:textAlignment="center"
        android:textSize="20dp"
        />

    <TextView
        android:id="@+id/votos_otimos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/votos_totais"
        android:layout_marginTop="50dp"
        android:textAllCaps="true"
        android:textStyle="italic"
        android:textAlignment="center"
        android:textSize="20dp"
        />

    <TextView
        android:id="@+id/votos_bons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/votos_otimos"
        android:layout_marginTop="50dp"
        android:textAllCaps="true"
        android:textStyle="italic"
        android:textAlignment="center"
        android:textSize="20dp"
        />

    <TextView
        android:id="@+id/votos_regulares"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/votos_bons"
        android:layout_marginTop="50dp"
        android:textAllCaps="true"
        android:textStyle="italic"
        android:textAlignment="center"
        android:textSize="20dp"
        />

    <TextView
        android:id="@+id/votos_ruins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/votos_regulares"
        android:layout_marginTop="50dp"
        android:textAllCaps="true"
        android:textStyle="italic"
        android:textAlignment="center"
        android:textSize="20dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>